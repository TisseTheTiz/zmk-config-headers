#define SYM_LT4 &trans           //
#define SYM_LT3 &trans           //
#define SYM_LT2 &trans           //
#define SYM_LT1 &trans           // ╭───────────────────────────────────────────────────────────────────────────────────────────────────╮
#define SYM_LT0 &trans           // │         │         │         │         │         │         │         │         │         │         │
#define SYM_RT0 &trans           // │ SYM_LT4 │ SYM_LT3 │ SYM_LT2 │ SYM_LT1 │ SYM_LT0 │ SYM_RT0 │ SYM_RT1 │ SYM_RT2 │ SYM_RT3 │ SYM_RT4 │
#define SYM_RT1 &trans           // │ ------- │ ------- │ ------- │ ------- │ ------- │ ------- │ ------- │ ------- │ ------- │ ------- │
#define SYM_RT2 &trans           // │         │         │         │         │         │         │         │         │         │         │
#define SYM_RT3 &trans           // │         │         │         │         │         │         │         │         │         │         │
#define SYM_RT4 &trans           // │         │         │         │         │         │         │         │         │         │         │
                                 // /         /         /         /         /         /         /         /         /         /         /
#define SYM_LM4 &trans           // /─────────/─────────/─────────/─────────/─────────/─────────/─────────/─────────/─────────/─────────/
#define SYM_LM3 &trans           // /         /         /         /         /         /         /         /         /         /         /
#define SYM_LM2 &trans           // │         │         │         │         │         │         │         │         │         │         │
#define SYM_LM1 &trans           // │ SYM_LM4 │ SYM_LM3 │ SYM_LM2 │ SYM_LM1 │ SYM_LM0 │ SYM_RM0 │ SYM_RM1 │ SYM_RM2 │ SYM_RM3 │ SYM_RM4 │
#define SYM_LM0 &trans           // │ ------- │ ------- │ ------- │ ------- │ ------- │ ------- │ ------- │ ------- │ ------- │ ------- │
#define SYM_RM0 &trans           // │         │         │         │         │         │         │         │         │         │         │
#define SYM_RM1 &trans           // │         │         │         │         │         │         │         │         │         │         │
#define SYM_RM2 &trans           // │         │         │         │         │         │         │         │         │         │         │
#define SYM_RM3 &trans           // /         /         /         /         /         /         /         /         /         /         /
#define SYM_RM4 &trans           // /─────────/─────────/─────────/─────────/─────────/─────────/─────────/─────────/─────────/─────────/
                                 // /         /         /         /         /         /         /         /         /         /         /
#define SYM_LB4 &kp EQUAL        // │         │         │         │         │         │         │         │         │         │         │
#define SYM_LB3 &kp PLUS         // │ SYM_LB4 │ SYM_LB3 │ SYM_LB2 │ SYM_LB1 │ SYM_LB0 │ SYM_RB0 │ SYM_RB1 │ SYM_RB2 │ SYM_RB3 │ SYM_RB4 │
#define SYM_LB2 &kp MINUS        // │ ------- │ ------- │ ------- │ ------- │ ------- │ ------- │ ------- │ ------- │ ------- │ ------- │
#define SYM_LB1 &kp UNDER        // │         │         │         │         │         │         │         │         │         │         │
#define SYM_LB0 &kp STAR         // │         │         │         │         │         │         │         │         │         │         │
#define SYM_RB0 &trans           // │         │         │         │         │         │         │         │         │         │         │
#define SYM_RB1 &trans           // ╰───────────────────────────────────────────────────────────────────────────────────────────────────╯
#define SYM_RB2 &trans           //
#define SYM_RB3 &trans           //
#define SYM_RB4 &trans           //

#define SYM_LH2 &trans           // ╭───────────────────────────────────────────────────────────────────────────────────────────────────╮
#define SYM_LH1 &kp PIPE         // │ ///////// SYM_LH3 ///////// │ SYM_LH1 │ SYM_LH0 < SYM_RH0 │ SYM_RH1 │ ///////// SYM_RH3 ///////// │
#define SYM_LH0 &my_lt NUM AMPS  // │ ///////// ------- ///////// │ ------- │ ------- > ------- │ ------- │ ///////// ------- ///////// │
#define SYM_RH0 &trans           // │ /////////         ///////// │         │         <         │         │ /////////         ///////// │
#define SYM_RH1 &trans           // │ /////////         ///////// │    |    │    &    >         │         │ /////////         ///////// │
#define SYM_RH2 &trans           // ╰───────────────────────────────────────────────────────────────────────────────────────────────────╯
